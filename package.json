{
  "name": "el-palleter",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:astro\" \"npm run dev:admin\" \"npm run dev:webhook\"",
    "dev:astro": "npm run download-images && astro dev",
    "dev:admin": "cd admin && npm run dev",
    "dev:webhook": "cd build-webhook && npm run dev",
    "download-images": "node scripts/download-images.js",
    "build": "npm run build:astro && npm run build:admin",
    "build:astro": "npm run download-images && astro build",
    "build:admin": "cd admin && npm run build",
    "preview": "astro preview",
    "astro": "astro",
    "install:all": "npm install && cd admin && npm install && cd build-webhook && npm install"
  },
  "dependencies": {
    "@astrojs/node": "^8.3.3",
    "@astrojs/react": "^3.6.2",
    "astro": "^4.15.9",
    "cookie": "^1.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@rollup/plugin-strip": "^3.0.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2"
  }
}
